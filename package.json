{
  "name": "node-circleci-autorelease",
  "version": "1.3.0",
  "description": "create release tags at CircleCI",
  "main": "bin/cc-generate-yml",
  "bin": {
    "cc-bmp": "bin/cc-bmp",
    "cc-generate-yml": "bin/cc-generate-yml"
  },
  "scripts": {
    "postinstall": "bin/cc-install",
    "test": "echo no test",
    "bmp-p": "cc-bmp -p",
    "bmp-m": "cc-bmp -m",
    "bmp-j": "cc-bmp -j",
    "circle": "cc-generate-yml",
    "post-dependencies": "echo post-dependencies",
    "pre-release": "echo pre-release",
    "post-release": "echo post-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CureApp/node-circleci-autorelease.git"
  },
  "author": "CureApp, Inc.",
  "license": "MIT",
  "dependencies": {
    "coffee-script": "^1.10.0",
    "deepmerge": "^0.2.10",
    "js-yaml": "^3.4.3"
  },
  "node-circleci-autorelease": {
    "config": {
      "git-user-name": "CircleCI",
      "git-user-email": "circleci@cureapp.jp",
      "version-prefix": "v"
    },
    "ignores": [
      "node_modules",
      ".editorconfig",
      "spec",
      ".releaseignore",
      ".bmp.yml",
      "npm-debug.log"
    ]
  }
}
