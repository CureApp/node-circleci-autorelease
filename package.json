{
  "name": "node-circleci-autorelease",
  "version": "2.0.3",
  "description": "create release tags at CircleCI",
  "main": "dist/bin/nca",
  "bin": {
    "nca": "dist/bin/nca.js"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "babel": "babel src -d dist",
    "lint": "eslint $(find src -type f ! -path '*/decls/*')",
    "flow": "flow",
    "test": "npm run lint && flow && npm run test:spec",
    "test:spec": "mocha $(find test/spec -type f ! -path '*/data/*')"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CureApp/node-circleci-autorelease.git"
  },
  "author": "CureApp, Inc.",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "deepmerge": "^0.2.10",
    "js-yaml": "^3.4.3",
    "shelljs": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.10.2",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-flowtype": "^2.2.7",
    "eslint-plugin-react": "^5.1.1",
    "flow-bin": "^0.25.0",
    "mocha": "^2.4.5",
    "power-assert": "^1.4.1"
  }
}
